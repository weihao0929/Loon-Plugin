#!name=Xmind Unlock (Token版)
#!desc=基于 chxm1023 脚本，包含 Token 和 Device 双重验证。
#!author=chxm1023 & Ayden
#!icon=https://raw.githubusercontent.com/weihao0929/icon_library/main/icon/Xmind.png

[Script]
# 核心脚本：匹配 devices 和 token 接口
http-response ^https?:\/\/.*xmind\..*\/_res\/(devices|token\/.*) script-path=https://raw.githubusercontent.com/weihao0929/Loon-Plugin/main/Xmind/xmind.js, requires-body=true, timeout=60, tag=Xmind_Unlock

[Mitm]
hostname = *.xmind.*, *.xmind.cn, *.xmind.net, *.xmind.app